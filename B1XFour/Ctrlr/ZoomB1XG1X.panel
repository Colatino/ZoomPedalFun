        <luaMethod luaMethodName="expandFileBlock" luaMethodCode="--[[&#13;&#10;Given a memory block mbIn&#13;&#10;return the processed block&#13;&#10;8 7bit values become 7 8 bit values&#13;&#10;--]]&#13;&#10;&#13;&#10;function expandFileBlock(validBytes, mbIn)&#10;&#9;local mbOut = MemoryBlock()&#13;&#10;&#9;local tMB = MemoryBlock()&#13;&#10;&#9;local loop = -1&#13;&#10;&#9;local hibits = 0&#13;&#10;    local inBlockSize = mbIn:getSize()&#13;&#10;    local chunks = floor(inBlockSize/8)&#13;&#10;    console(&quot;inBlockSize = &quot;..inBlockSize)&#13;&#10;    console(&quot;chunks = &quot;..chunks)&#13;&#10;&#9;console(&quot;validBytes = &quot;..validBytes)&#13;&#10;&#13;&#10;&#9;if chunks * 8 ~= inBlockSize then&#13;&#10;        console(&quot;Hmmh ... is this block size correct?&quot;)&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;--[[&#13;&#10;&#9;&#9;take 7bit values from memory block at a time.&#13;&#10;&#9;&#9;Seem to think&#13;&#10;&#9;&#9;In&#13;&#10;&#9;&#9;0  1  2  3  4  5  6  7&#13;&#10;&#9;&#9;Hi v6 v5 v4 v3 v2 v1 v0&#13;&#10;&#13;&#10;&#9;&#9;&#9;&#9;6 5 4 3 2 1 0  bit&#13;&#10;&#9;&#9;Hi = (  &#94; &#94; &#94; &#94; &#94; &#94; &#94;)&#13;&#10;&#13;&#10;&#9;&#9;Out&#13;&#10;&#9;&#9;0  1  2  3  4  5  6&#13;&#10;&#9;&#9;In(1)+128*(Hi(bit6))&#13;&#10;&#9;&#9;   In(2)+128*(Hi(bit5))&#13;&#10;              In(3)+128*(Hi(bit4))&#13;&#10;&#9;&#9;         In(4)+128*(Hi(bit3))&#13;&#10;&#9;&#9;            In(5)+128*(Hi(bit2))&#13;&#10;&#9;&#9;               In(6)+128*(Hi(bit1))&#13;&#10;&#9;&#9;                  In(7)+128*(Hi(bit0))&#13;&#10;&#9;--]]&#13;&#10;&#9;local outByteCounter = 0&#13;&#10;&#9;local procVal = 0&#13;&#10;&#9;-- validBytes is the number of 8 bit values.&#13;&#10;&#9;-- we need 8 to make 7 valid bytes&#13;&#10;&#9;-- so we need validBytes * 8/7 = validBytes*(1 + 1/7)&#13;&#10;&#9;for i = 0, ceil(inBlockSize/7)*7, 8&#13;&#10; &#9;do&#13;&#10;&#9;&#9;hibits = mbIn:getByte(i)&#13;&#10;&#9;&#9;-- console(&quot;Starting processing &quot;..i..&quot; hibits&quot;..hibits)&#13;&#10;&#13;&#10;&#9;&#9;if hibits == 0 then&#13;&#10;&#9;&#9;&#9;-- console(&quot;shortcut&quot;)&#13;&#10;&#9;&#9;&#9;for j = 1, 7&#13;&#10;&#9;&#9;&#9;do&#13;&#10;&#9;&#9;&#9;&#9;procVal = mbIn:getByte(i+j)&#13;&#10;&#9;&#9;&#9;&#9;tMB:loadFromHexString( string.format(&quot;%.2X&quot;, procVal))&#10;&#9;&#9;&#9;&#9;mbOut:append(tMB)&#13;&#10;            &#9;outByteCounter = outByteCounter + 1&#13;&#10;&#9;&#9;&#9;&#9;if outByteCounter == validBytes then&#13;&#10;&#9;&#9;&#9;&#9;&#9;break&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;-- otherwise we gotta process&#13;&#10;&#9;&#9;&#9;console(&quot;inner loop with hibits = &quot;..hibits)&#13;&#10;&#9;&#9;&#9;for j = 1, 7&#13;&#10;        &#9;do&#13;&#10;&#9;&#9;&#9;&#9;-- console(&quot;\tStarting processing &quot;..j..&quot;  &quot;..(i+j)..&quot; hitbit set? &quot;..BitAND(hibits, 2&#94;(7-j)))&#13;&#10;&#9;&#9;&#9;&#9;if BitAND(hibits, 2&#94;(7-j)) ~= 0 then&#13;&#10;&#9;&#9;&#9;&#9;&#9;procVal = mbIn:getByte(i+j)&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;      BINGO: &quot;..j)&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;&quot;..(i+j))&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;WAS &quot;..procVal)&#13;&#10;&#9;&#9;&#9;&#9;&#9;procVal = procVal + 128&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;NOW &quot;..procVal)&#13;&#10;&#9;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;&#9;procVal = mbIn:getByte(i+j)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;tMB:loadFromHexString( string.format(&quot;%.2X&quot;, procVal))&#10;&#9;&#9;&#9;&#9;mbOut:append(tMB)&#13;&#10;            &#9;outByteCounter = outByteCounter + 1&#13;&#10;&#9;&#9;&#9;&#9;if outByteCounter == validBytes then&#13;&#10;&#9;&#9;&#9;&#9;&#9;break&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;    end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;console(&quot;OutByteCounter = &quot;..outByteCounter)&#13;&#10;    console( mbIn:toHexString(1) )&#13;&#10;&#9;-- console( mbOut:toHexString(1) )&#13;&#10;&#9;return mbOut&#13;&#10;end"
                   luaMethodLinkedProperty="" luaMethodSource="0" uuid="5d98c60faa8140e391c9f0544a2cb45f"
                   luaMethodValid="1"/>
        <luaMethod luaMethodName="processFLSTContent" luaMethodCode="function processFLSTContent()&#10;&#9;--[[&#13;&#10;&#9;So we are looking for blocks of headers then content&#13;&#10;&#9;Zoom uses 3e 3e 3e (&lt;&lt;&lt;) and 3c 3c 3c (&gt;&gt;&gt;) to delimit&#13;&#10;&#9;But each record is 26 bytes ... ascii with string termination?&#13;&#10;&#9;Count 3e3e3e.&#13;&#10;&#9;Begin =&gt;0 .. so nothing&#13;&#10;&#9;find 3e3e3e ++ means 1 ie catalog&#13;&#10;&#9;read catalog header&#13;&#10;&#9;find 3e3e3e ++ means 2 ie FX GROUP&#13;&#10;&#9;read group header&#13;&#10;&#9;find 3c3c3c -&gt; next, so 3e3e3e --&#13;&#10;&#9;read group ending header -- compare with group starting header&#13;&#10;&#9;ie 2 -&gt; 1, so we back in catalog&#13;&#10;&#9;find 3e3e3e ++ means 2 ie FX GROUP&#13;&#10; &#9;read group header&#13;&#10;&#9;keep reading rows (check there is space for another row)&#13;&#10;&#13;&#10;&#9;when back to end catalog&#13;&#10;&#9;if 00 00 or end of bytes then we at end of catalog&#13;&#10;&#13;&#10;&#9;within a record the values are \0 delimited.&#13;&#10;&#9;12 bytes name \0 &#9;&#9;&#9;= 13 bytes&#13;&#10;&#9;4 bytes ascii version \0 &#9;= 5 bytes&#13;&#10;&#9;2 bytes FX ID?&#9;\0&#9;&#9;&#9;= 3 bytes&#13;&#10;&#9;2 bytes 80|0 GID ... I think first is if installed&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;= 3 bytes&#13;&#10;&#9;2 bytes empty&#9;&#9;&#9;&#9;= 2 bytes&#13;&#10;&#9;---&#13;&#10;&#9;26 bytes&#13;&#10;&#13;&#10;&#9;--]]&#13;&#10;&#13;&#10;&#9;g_FXList = nil&#13;&#10;&#9;g_FXList = {}&#13;&#10;&#13;&#10;&#9;-- get the mod we want to update&#13;&#10;&#9;FX = panel:getModulatorByName (&quot;FX&quot;)&#13;&#10;&#9;cFX = FX:getComponent()&#13;&#10;&#9;comboValues =&quot;&quot;&#13;&#10;--[[ :getModulatorValue()&#13;&#10;   panel:getModulatorByName (&quot;multiTab&quot;):setModulatorValue(vvv, false, false, false)&#13;&#10;uiComboSelectedIndex=&quot;-1&quot; &#13;&#10;uiComboContent=&quot;EMPTY&quot;&#13;&#10;    comp:setPropertyString(&quot;uiLabelText&quot;, &quot;Load&#13;&#10;uiComboContent=&quot;EMPTY&amp;#10;v1&amp;#10;v2&quot;&#13;&#10;uiComboContent=&quot;genius&quot;&#13;&#10;&#13;&#10;multiTab use this to determine which tab, and hence whether to write multi or single.&#13;&#10;   panel:getModulatorByName (&quot;multiTab&quot;):getModulatorValue()&#13;&#10;   panel:getModulatorByName (&quot;multiTab&quot;):setModulatorValue(vvv, false, false, false)&#13;&#10;&#13;&#10;    comp:setPropertyString(&quot;uiLabelText&quot;, &quot;Load All..&quot;)&#13;&#10;    panel:getModulatorByName (&quot;multiTab&quot;):setModulatorValue(vvv, false, false, false)&#13;&#10;comp:getOwner():getModulatorValu&#13;&#10;FX&#13;&#10;--]]&#13;&#10;&#9;local ThreeE= 0&#13;&#10;&#13;&#10;&#9;local contentSize = g_procZT2_mb:getSize()&#13;&#10;&#9;--console( g_procZT2_mb:toHexString(1) )&#13;&#10;&#9;local currentByteLoc = 0&#13;&#10;&#9;local finished = false&#13;&#10;&#9;local recordLength = 26&#13;&#10;&#9;local recordCount = 0&#13;&#10;&#9;local compareGroupString&#13;&#10;&#9;local compareFolderString&#13;&#10;&#9;while ( (finished == false) and (currentByteLoc &lt; (contentSize - recordLength) ) )&#13;&#10;&#9;do&#13;&#10;&#9;&#9;recordCount = recordCount + 1&#13;&#10;&#9;&#9;local currentRecord = g_procZT2_mb:getRange(currentByteLoc, recordLength)&#13;&#10;&#9;&#9;currentByteLoc = currentByteLoc + recordLength&#13;&#10;&#13;&#10;&#9;&#9;local name = currentRecord:getRange(0, 13) -- should be nil terminated&#13;&#10;&#9;&#9;local version = currentRecord:getRange(13, 5) -- should be nil terminated&#13;&#10;&#9;&#9;local FXID = currentRecord:getRange(18, 2) -- should be nil terminated. 3&#13;&#10;&#9;&#9;local GID = currentRecord:getRange(21, 2) -- should be nil terminated. 3&#13;&#10;&#13;&#10;&#9;&#9;if (name:getByte(0) == 0x3e &#13;&#10;&#9;&#9;&#9;and name:getByte(1) == 0x3e &#13;&#10;&#9;&#9;&#9;and name:getByte(2) == 0x3e &#13;&#10;&#9;&#9;&#9;and name:getByte(3) == 0x00) then&#13;&#10;&#13;&#10;&#9;&#9;&#9;ThreeE = ThreeE + 1&#13;&#10;&#9;&#9;&#9;if ThreeE == 1 then&#13;&#10;&#9;&#9;&#9;&#9;compareFolderString = currentRecord:getRange(4, 22)&#13;&#10;&#9;&#9;&#9;elseif ThreeE == 2 then&#13;&#10;&#9;&#9;&#9;&#9;compareGroupString = currentRecord:getRange(4, 22)&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;elseif (name:getByte(0) == 0x3c &#13;&#10;&#9;&#9;&#9;and name:getByte(1) == 0x3c &#13;&#10;&#9;&#9;&#9;and name:getByte(2) == 0x3c &#13;&#10;&#9;&#9;&#9;and name:getByte(3) == 0x00) then&#13;&#10;&#9;&#9;&#9;ThreeE = ThreeE - 1&#13;&#10;&#9;&#9;&#9;if ThreeE == 1 then&#13;&#10;&#9;&#9;&#9;&#9;local check = currentRecord:getRange(4, 22)&#13;&#10;&#9;&#9;&#9;&#9;same = true&#13;&#10;&#9;&#9;&#9;&#9;i = 0&#13;&#10;&#9;&#9;&#9;&#9;while ((same == true) and (i &lt; 22))&#13;&#10;&#9;&#9;&#9;&#9;do&#13;&#10;&#9;&#9;&#9;&#9;&#9;same = same and (check:getByte(i) == compareFolderString(i))&#13;&#10;&#9;&#9;&#9;&#9;&#9;i = i + 1&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;if ((same == true) and (i == 22)) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;Folder integrity good&quot;)&#13;&#10;&#9;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;Folder integrity POOR&quot;)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;compareFolderString = currentRecord:getRange(4, 22)&#13;&#10;&#9;&#9;&#9;elseif ThreeE == 2 then&#13;&#10;&#9;&#9;&#9;&#9;local check = currentRecord:getRange(4, 22)&#13;&#10;&#9;&#9;&#9;&#9;same = true&#13;&#10;&#9;&#9;&#9;&#9;i = 0&#13;&#10;&#9;&#9;&#9;&#9;while ((same == true) and (i &lt; 22))&#13;&#10;&#9;&#9;&#9;&#9;do&#13;&#10;&#9;&#9;&#9;&#9;&#9;same = same and (check:getByte(i) == compareGroupString(i))&#13;&#10;&#9;&#9;&#9;&#9;&#9;i = i + 1&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;if ((same == true) and (i == 22)) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;Group integrity good&quot;)&#13;&#10;&#9;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;&#9;console(&quot;Group integrity POOR&quot;)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;compareGroupString = currentRecord:getRange(4, 22)&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;elseif (name:getByte(0) == 0x0 &#13;&#10;&#9;&#9;&#9;and name:getByte(1) == 0x0) -- probably need to check more but cheat for moment&#13;&#10;&#9;&#9;&#9;&#9;then&#13;&#10;&#9;&#9;&#9;-- we must be at end of file ...&#13;&#10;&#9;&#9;&#9;if ThreeE ~= 0 then&#13;&#10;&#9;&#9;&#9;&#9;console(&quot;hmmh ... I expected us to be back to normal!!&quot;)&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;finished = true&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;-- ok record should be good .. print it&#13;&#10;&#9;&#9;&#9;-- L(d:getRange(8,17):toString())&#13;&#10;&#9;&#9;&#9;FXName = L( name:toString() )&#13;&#10;&#9;&#9;&#9;console( FXName )&#13;&#10;&#9;&#9;&#9;FXVersion = L( version:toString() )&#13;&#10;&#9;&#9;&#9;console( FXVersion )&#13;&#10;&#9;&#9;&#9;FXIDint = FXID:getByte(1)&#13;&#10;&#9;&#9;&#9;GID1 = GID:getByte(0)&#13;&#10;&#9;&#9;&#9;GID2 = GID:getByte(1)&#13;&#10;&#9;&#9;&#9;-- needs to be divided by 32&#13;&#10;&#9;&#9;&#9;FXGIDint = (GID2 * 256 + GID1) / 32&#13;&#10;&#9;&#9;&#9;console( &quot;FXID: &quot;..string.format(&quot;%.2X&quot;,FXIDint)..&quot; int: &quot;..string.format(&quot;%d&quot;,FXIDint) )&#13;&#10;&#9;&#9;&#9;console( &quot;GID: &quot;..string.format(&quot;%.2X%.2X&quot;,GID2, GID1)..&quot; int: &quot;..string.format(&quot;%d&quot;,FXGIDint) )&#13;&#10;&#9;&#9;&#9;table.insert(g_FXList, { FXName, FXVersion, FXIDint, FXGIDint})&#13;&#10;&#9;&#9;&#9;comboValues = comboValues..FXName..&quot;\n&quot;&#13;&#10;&#9;&#9;end&#9;&#13;&#10;&#9;end&#9;&#13;&#10;&#9;cFX:setPropertyString(&quot;uiComboContent&quot;, comboValues)&#13;&#10;&#13;&#10;&#9;console(&quot;Read &quot;..recordCount..&quot; records&quot;)&#13;&#10;end"
                   luaMethodLinkedProperty="" luaMethodSource="0" uuid="130d32c4f5034966a204d5f56b7f2280"
                   luaMethodValid="1"/>
        <luaMethod luaMethodName="updateFXLCDs" luaMethodCode="--&#10;-- Called when a modulator value changes&#10;-- @mod   http://ctrlr.org/api/class_ctrlr_modulator.html&#10;-- @value    new numeric value of the modulator&#10;--&#10;updateFXLCDs = function(--[[ CtrlrModulator --]] mod, --[[ number --]] value, --[[ number --]] source)&#10;&#13;&#10;&#9;debugConsole(&quot;In updateFXLCDs&quot;)&#13;&#10;&#13;&#10;&#9;local&#9;val = 1 + mod:getModulatorValue()&#13;&#10;&#13;&#10;&#9;debugConsole(&quot;&quot;..val)&#13;&#10;&#9;lcd = {}&#13;&#10;&#9;lcd[1] = panel:getModulatorByName (&quot;lcdFXFile&quot;)&#13;&#10;&#9;lcd[2] = panel:getModulatorByName (&quot;lcdFXVersion&quot;)&#13;&#10;&#9;lcd[3] = panel:getModulatorByName (&quot;lcdFXID&quot;)&#13;&#10;&#9;lcd[4] = panel:getModulatorByName (&quot;lcdFXGID&quot;)&#13;&#10;&#13;&#10;&#9;cFX = {}&#13;&#10;&#13;&#10;&#9;cFX[1] = lcd[1]:getComponent()&#13;&#10;&#9;cFX[2] = lcd[2]:getComponent()&#13;&#10;&#9;cFX[3] = lcd[3]:getComponent()&#13;&#10;&#9;cFX[4] = lcd[4]:getComponent()&#13;&#10;&#13;&#10;&#9;for i = 1, 2&#13;&#10;&#9;do&#13;&#10;&#9;&#9;if g_FXList[val] ~= nil and g_FXList[val][i] ~= nil then&#13;&#10;&#9;&#9;&#9;console(&quot;&quot;..g_FXList[val][i])&#13;&#10;&#9;&#9;&#9;cFX[i]:setPropertyString(&quot;uiLabelText&quot;, g_FXList[val][i])&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;for i = 3, 4&#13;&#10;&#9;do&#13;&#10;&#9;&#9;if g_FXList[val] ~= nil and g_FXList[val][i] ~= nil then&#13;&#10;&#9;&#9;&#9;console(&quot;&quot;..g_FXList[val][i])&#13;&#10;&#9;&#9;&#9;cFX[i]:setPropertyString(&quot;uiLabelText&quot;, string.format(&quot;%d&quot;, g_FXList[val][i]))&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;"
                   luaMethodLinkedProperty="luaModulatorValueChange" luaMethodSource="0"
                   uuid="63b9a7be7db64c24b93e8b091a6b02e4" luaMethodValid="1"/>
      </luaMethodGroup>
