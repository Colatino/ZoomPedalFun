      <luaMethodGroup name="Sysex" uuid="027cc6d906d04c9db64656de25bfeb1c">
        <luaMethod luaMethodName="handlePanelMidiMessage" luaMethodCode="--&#13;&#10;-- Called when a panel receives a midi message (does not need to match any modulator mask)&#13;&#10;-- @midi   CtrlrMidiMessage object&#13;&#10;--&#13;&#10;&#13;&#10;handlePanelMidiMessage = function(--[[ CtrlrMidiMessage --]] midiMessage)&#13;&#10;&#9;if panel:getRestoreState() == true or panel:getProgramState() == true then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10; &#13;&#10;-- So we get to choose how we respond to received messages.&#13;&#10;-- This is likely critical on the Zoom since Sysex can&#13;&#10;-- trigger Bank change etc.&#13;&#10;&#13;&#10;    local size = midiMessage:getSize()&#13;&#10;    debugConsole(&quot;handlePanelMidiMessage &quot;..size)&#13;&#10;    debugConsole(tostring(size))&#13;&#10;    local d = midiMessage:getData()&#13;&#10;&#9;-- debugConsole(d:toHexString(1))&#13;&#10;&#13;&#10;-- need to create a state machine to describe sysex transactions.&#13;&#10;&#9;local x = receivedZT2FileLength(d)&#13;&#10;--[[&#13;&#10;&#9;if g_ReceivingZT2 and x then&#13;&#10;&#9;&#9;console(&quot;\tSOMETHING WRONG ... we are receiving file twice&quot;)&#13;&#10;&#9;else if g_ReceivingZT2 then&#13;&#10;&#13;&#10;&#9;else&#13;&#10;&#9;end&#13;&#10;--]]&#13;&#10;&#13;&#10;&#9;local pd = receivedPatchDump(d)&#13;&#10;&#13;&#10;&#9;local zr = zoomOKResponse(d)&#13;&#10;&#13;&#10;&#9;local zfr = zoomFlushSysexOK(d)&#13;&#10;&#13;&#10;--[[&#13;&#10;&#13;&#10;SO SOME OF THE PACKETS ARE BROKEN INTO 256 BYTE CHUNKS ...&#13;&#10;THE NEXT PACKET WONT BE F0 ... BUT WE WANT TO CARRY ON UNTIL&#13;&#10;WE GET TO THE F7 .. think about how to do this.&#13;&#10;&#13;&#10;--]]&#13;&#10;&#13;&#10;--[[we need to know if we want to append this data to a ZT2 capture&#13;&#10;    globally ZT2 size is set to 0, and each time we try to read&#13;&#10;&#9;a new one. So if not 0 we are trying to add data&#13;&#10;&#9;g_mb = MemoryBlock()&#13;&#10;&#9;g_ZT2_mb = MemoryBlock()&#13;&#10;--]]&#13;&#10;&#9;local blockSize = checkZT2BlockSize(d)&#13;&#10;&#13;&#10;&#9;local blockData = getZT2BlockData(d)&#13;&#10;&#13;&#10;&#9;local foOK = fileOpenOK(d)&#13;&#10;&#13;&#10;&#9;if g_ZT2_size ~= 0 then&#13;&#10;&#9;&#9;-- ok so we are processing a ZT2&#13;&#10;&#9;&#9;-- check we find correct message, append subset&#13;&#10;    &#9;if &#9;&#9;d:getByte(0) == 0xF0 and d:getByte(1) == 0x52&#13;&#10; &#9;&#9;&#9;and d:getByte(2) == 0x00 and d:getByte(3) == 0x6e&#13;&#10;&#9;&#9;&#9;and d:getByte(4) == 0x60 and d:getByte(5) == 0x04&#13;&#10;&#9;&#9;&#9;and d:getByte(6) == 0x22 and d:getByte(7) == 0x01&#13;&#10;&#9;&#9;&#9;and g_ReceivingZT2 == true then&#13;&#10;&#9;&#9;&#9;-- so we want to remove 1st 11 bytes&#13;&#10;&#9;&#9;&#9;-- and take whole blocks of 8 for 8 -&gt; 7 processing&#13;&#10;&#9;&#9;&#9;if d:getByte(size-1) ~= 0xF7 then&#13;&#10;&#9;&#9;&#9;&#9;console(&quot;\t\t Not sure why last byte isnt F7&quot;)&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;local validBytes = d:getByte(9) + 128 * d:getByte(10)&#13;&#10;&#9;&#9;&#9;console(&quot;validBytes = &quot;..validBytes)&#13;&#10;&#9;&#9;&#9;if validBytes &gt; 0 then&#13;&#10;&#9;&#9;&#9;&#9;g_receivedBytes = g_receivedBytes + validBytes&#13;&#10;&#9;&#9;&#9;&#9;local byteChunk = validBytes + floor(validBytes/7) + 1&#13;&#10;&#9;&#9;&#9;&#9;g_mb:append(d:getRange(11, byteChunk))&#13;&#10;        &#9;&#9;console(&quot;\tAdding more cruft&quot;)&#13;&#10;        &#9;&#9;debugConsole (string.format(&quot;\t\tSysexChunk size: %d as data: %s&quot;, size, d:toHexString(1)) )&#13;&#10;&#13;&#10;&#9;&#9;&#9;&#9;-- now we want to process largest 8 byte chunk we can.&#13;&#10;&#9;&#9;&#9;&#9;-- prevLast = g_ZT2_lastPosition&#13;&#10;&#9;&#9;&#9;&#9;-- g_ZT2_lastPosition = floor(byteChunk/8)*8 + g_ZT2_lastPosition&#13;&#10;&#13;&#10;&#9;&#9;&#9;&#9;local mbOut = expandFileBlock( validBytes, d:getRange(11, byteChunk) )&#13;&#10;&#9;&#9;&#9;&#9;console(&quot;Processed &quot;..mbOut:getSize())&#13;&#10;&#9;&#9;&#9;&#9;console(mbOut:toHexString(1))&#13;&#10;&#9;&#9;&#9;&#9;g_procZT2_mb:append(mbOut)&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;console(&quot;\t\t\tFOUND END OF FILE&quot;)&#13;&#10;&#9;&#9;&#9;&#9;console(&quot;End of file upload. You can process it now&quot;)&#13;&#10;&#9;&#9;&#9;&#9;console(&quot;Received &quot;..g_receivedBytes)&#13;&#10;&#9;&#9;&#9;&#9;g_ReceivingZT2 = false&#13;&#10;&#9;&#9;&#9;&#9;processFLSTContent()&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;if not(x or pd or zr or zfr or (g_ZT2_size == 0) or foOK) then&#13;&#10;        debugConsole(&quot;not sysex!&quot;)&#13;&#10;        debugConsole (string.format(&quot;SysexLoad size: %d as data: %s&quot;, size, d:toHexString(1)))&#13;&#10;    end&#13;&#10;end"
                   luaMethodLinkedProperty="" luaMethodSource="0" uuid="9a5e75a1132f44cc94a953c85dcf1cb9"
                   luaMethodValid="1"/>
        <luaMethod luaMethodName="requestFLST" luaMethodCode="-- So we need to send a series of sysex requests and store data&#13;&#10;-- but each time we receive sysex we will go thru handlePanelMidiMessage&#13;&#10;-- so we need to flag to this processor that we are in mid flow?&#13;&#10;function requestFLST(--[[ CtrlrModulator --]] mod, --[[ number --]] value, --[[ number --]] source)&#10;&#10;    debugConsole(&quot;requestFLST()&quot;)&#13;&#10;&#9;if panel:getRestoreState() == true or panel:getProgramState() == true then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;-- we have to be in PCMode and EditorMode&#13;&#10;    PCModeOn()&#13;&#10;&#13;&#10;-- ask if file exists?&#13;&#10;&#9;name = &quot;FLST_SEQ.ZT2&quot;&#13;&#10;&#9;g_ReceivingZT2 = true&#13;&#10;&#9;g_ZT2_size = 0&#13;&#10;&#9;g_receivedBytes = 0&#13;&#10;&#13;&#10;&#9;g_ZT2_blocksize = 0&#13;&#10;&#9;g_ZT2_lastPosition = 0&#13;&#10;&#9;g_mb = nil&#13;&#10;&#9;g_mb = MemoryBlock()&#13;&#10;&#9;g_ZT2_mb = nil&#13;&#10;&#9;g_ZT2_mb = MemoryBlock()&#13;&#10;&#9;g_procZT2_mb = nil&#13;&#10;    g_procZT2_mb = MemoryBlock()&#13;&#10;&#13;&#10;&#9;doesZT2FileExist(name)&#13;&#10;    -- addDelayMS(100)&#13;&#10;    flushSysex()&#13;&#10;    -- addDelayMS(100)&#13;&#10;&#13;&#10;&#9;initZT2Search()&#13;&#10;    -- addDelayMS(100)&#13;&#10;&#9;openZT2File(name)&#13;&#10;    -- addDelayMS(100)&#13;&#10;&#13;&#10;--    local size = midiMessage:getSize()&#13;&#10;--    debugConsole(&quot;requestFLST &quot;..size)&#13;&#10;&#13;&#10;&#9;for i = 1, 36 do&#13;&#10;        console(&quot;&quot;..i)&#13;&#10;&#9;&#9;flushSysex()&#13;&#10;    -- addDelayMS(100)&#13;&#10;&#9;&#9;getZT2Block()&#13;&#10;    -- addDelayMS(100)&#13;&#10;&#9;&#9;flushSysex()&#13;&#10;    -- addDelayMS(100)&#13;&#10;&#9;end&#13;&#10;&#9;g_ReceivingZT2 = false&#13;&#10;&#9;PCModeOff()&#13;&#10;--[[&#13;&#10;NOTE: the sends seem to be processed before the receives&#13;&#10;so my idea of setting g_ReceivingZT2 true then false is potentially wrong&#13;&#10;because the input finishes -- the receive therefore thinks nothing is&#13;&#10;happening&#13;&#10;So I need to process in the handlePanelMididMessage function&#13;&#10;swapping state with some globals.&#13;&#10;At end fo process we have&#13;&#10;&#13;&#10;&gt;&gt;&gt; console( String(g_mb:getSize()) )&#10;5608&#10;&#10;&gt;&gt;&gt; mbUnpackedData = utils.unpackDsiData(g_mb )&#10;&#10;&gt;&gt;&gt; console( String(mbUnpackedData:getSize()) )&#10;4907&#10;&#10;&gt;&gt;&gt; console( mbUnpackedData:toHexString(1) )&#13;&#10;&#13;&#10;and this seems to be PRETTY CLOSE to what we want!!&#13;&#10;Nice.&#13;&#10;--]]&#13;&#10;end"
                   luaMethodLinkedProperty="" luaMethodSource="0" uuid="8f675b24048844f39dc41e6018c3ec62"
                   luaMethodValid="1"/>
        <luaMethod luaMethodName="doesZT2FileExist" luaMethodCode="function doesZT2FileExist(name)&#13;&#10;-- should check name for sanity!!&#10;    debugConsole(&quot;doesZT2FileExist()&quot;)&#13;&#10;&#9;if panel:getRestoreState() == true or panel:getProgramState() == true then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;    -- careful - leave a space for concatenation&#13;&#10;&#9;local baseSysex = &quot;f0 52 00 6e 60 25 00 00 &quot;&#13;&#10;&#9;for i=1, string.len(name) do&#13;&#10;        baseSysex = baseSysex..&quot;&quot;..string.format(&quot;%02x &quot;, string.byte(name,i) )&#13;&#10;    end&#13;&#10;&#13;&#10;    baseSysex = baseSysex..&quot;00 f7&quot;&#13;&#10;    debugConsole(baseSysex)&#13;&#10;    panel:sendMidiMessageNow(CtrlrMidiMessage(baseSysex))&#13;&#10;end"
