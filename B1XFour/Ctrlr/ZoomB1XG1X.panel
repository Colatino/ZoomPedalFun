        <luaMethod luaMethodName="printFileOutput" luaMethodCode="function printFileOutput()&#10;&#9;console( String(g_mb:getSize()) )&#10;--&#9;mbUnpackedData = utils.unpackDsiData(g_mb )&#10;--&#9;console( String(mbUnpackedData:getSize()) )&#10;--&#9;console( mbUnpackedData:toHexString(1) )&#13;&#10;&#9;console( String( g_procZT2_mb:getSize() ))&#13;&#10;&#9;console( g_procZT2_mb:toHexString(1) )&#13;&#10;&#13;&#10;end"
