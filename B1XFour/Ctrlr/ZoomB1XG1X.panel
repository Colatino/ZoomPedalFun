        <luaMethod luaMethodName="receivedZT2FileLength" luaMethodCode="function receivedZT2FileLength(d)&#13;&#10;&#9;debugConsole(&quot;receivedZT2FileLength&quot;)&#13;&#10;    retval = false&#10;    if &#9;&#9;d:getByte(0) == 0xF0 and d:getByte(1) == 0x52&#13;&#10; &#9;&#9;and d:getByte(2) == 0x00 and d:getByte(3) == 0x6e&#13;&#10;&#9;&#9;and d:getByte(4) == 0x60 and d:getByte(5) == 0x04&#13;&#10;&#9;&#9;and d:getByte(6) == 0x25  then&#13;&#10;&#9;&#9;g_ReceivingZT2 = true  -- buffering can mean inputs proc before outputs?&#13;&#10;        local lowlen = d:getByte(30)&#13;&#10;&#9;&#9;local highlen = d:getByte(31)&#13;&#10;&#9;&#9;g_ZT2_size = lowlen + 128 * highlen&#13;&#10;        console(&quot;\tFileLen = &quot;..g_ZT2_size)&#13;&#10;&#9;&#9;if g_receivedBytes ~= 0 then&#13;&#10;&#9;&#9;&#9;console(&quot;\tWARNING - truncating??&quot;)&#13;&#10;&#9;&#9;end&#13;&#10;    &#9;g_receivedBytes = 0&#13;&#10;    &#9;g_procZT2_mb = nil&#13;&#10;    &#9;g_procZT2_mb = MemoryBlock()&#13;&#10;&#9;&#9;retval = true&#13;&#10;&#9;end&#13;&#10;&#9;return retval&#10;end"
